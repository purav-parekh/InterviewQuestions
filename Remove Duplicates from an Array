n=7
arr=[1,2,2,2,2,3,2]
#6
#56 42 21 23 65 20

def removeDuplicates(arr,n):
    b=[]
    a=arr[0]
    b.append(a)
    for i in range(1,n):
      print(i)
      if arr[i]!=arr[i-1]:
        b.append(arr[i])

    for i in range(len(b)):
      print(b[i],end=" ")

removeDuplicates(arr,n)       

